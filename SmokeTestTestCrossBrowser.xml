<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="WebDriver University Exercise All  Test Run" >
    <test verbose="2" preserve-order="true" name="WebDriver University Exercise All Test ui chrome" >
        <parameter name="browser" value="chrome"></parameter>
        <classes>
            <class name="tests.ui.webDriverUniverty.ContactUs"></class>
            <class name="tests.ui.webDriverUniverty.DataTableButton"></class>
            <class name="tests.ui.webDriverUniverty.Iframe"></class>


        </classes>
    </test>

    <test verbose="2" preserve-order="true" name="WebDriver University Exercise All Test ui firefox" >
        <parameter name="browser" value="edge"></parameter>
        <classes>
            <class name="tests.ui.webDriverUniverty.ContactUs"></class>
            <class name="tests.ui.webDriverUniverty.DataTableButton"></class>
            <class name="tests.ui.webDriverUniverty.Iframe"></class>

        </classes>
    </test>



</suite>
